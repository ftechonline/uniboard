<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="com.ftechonline.uniboard.MainActivity"
    tools:showIn="@layout/activity_main">

    <ImageView
        android:id="@+id/btnLogic"
        android:layout_width="302dp"
        android:layout_height="172dp"
        android:layout_centerHorizontal="true"
        android:layout_centerVertical="true"
        app:srcCompat="@drawable/esp"
        tools:layout_editor_absoluteX="41dp"
        tools:layout_editor_absoluteY="169dp" />

    <Button
        android:id="@+id/btnOutput"
        android:layout_width="300dp"
        android:layout_height="80dp"
        android:layout_alignEnd="@+id/btnLogic"
        android:layout_alignParentBottom="true"
        android:layout_alignRight="@+id/btnLogic"
        android:layout_marginBottom="44dp"
        android:text="Output"
        tools:layout_editor_absoluteX="42dp"
        tools:layout_editor_absoluteY="376dp" />

    <Button
        android:id="@+id/btnInput"
        android:layout_width="300dp"
        android:layout_height="80dp"
        android:layout_alignEnd="@+id/btnLogic"
        android:layout_alignParentTop="true"
        android:layout_alignRight="@+id/btnLogic"
        android:layout_marginTop="40dp"
        android:text="Input / Sensors"
        tools:layout_editor_absoluteX="38dp"
        tools:layout_editor_absoluteY="53dp" />
</RelativeLayout>
